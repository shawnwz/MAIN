{
    "extends": "eslint:recommended",
    "options": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "globals": [
        "$",
        "window",
        "o5",
        "$i",
        "$N",
        "CCOM",
        "document",
        "self",
        "jasmine",
        "expect",
        "it",
        "beforeAll",
        "describe",
        "jasmineRequire",
        "$util",
        "setTimeout",
        "clearTimeout",
        "setInterval",
        "$model",
        "XMLHttpRequest",
        "Set",
        "$actions",
        "$config",
        "$service",
        "app",
        "Promise",
        "console"
    ],
    "rules": {
        "no-alert": 2,
        "no-array-constructor": 1,
        "no-bitwise": 1,
        "no-caller": 1,
        "no-case-declarations": 2,
        "no-catch-shadow": 2,
        "no-class-assign": 2,
        "no-cond-assign": 2,
        "no-confusing-arrow": 2,
        "no-console": 0,
        "no-const-assign": 2,
        "no-constant-condition": 1,
        "no-continue": 0,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 2,
        "no-dupe-class-members": 2,
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-duplicate-case": 2,
        "no-else-return": 1,
        "no-empty": 1,
        "no-empty-character-class": 2,
        "no-empty-function": 0,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-label": 2,
        "no-extra-parens": 0,
        "no-extra-semi": 2,
        "no-fallthrough": 1,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implicit-coercion": 2,
        "no-implicit-globals": 2,
        "no-implied-eval": 2,
        "no-inline-comments": 0,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-invalid-this": 0,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 1,
        "no-loop-func": 2,
        "no-mixed-requires": 2,
        "no-mixed-spaces-and-tabs": 0,
        "linebreak-style": 2,
        "no-multi-spaces": 0,
        "no-multi-str": 0,
        "no-multiple-empty-lines": [1, {
                "max": 4
            }],
        "no-native-reassign": 2,
        "no-negated-condition": 0,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 0,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-require": 2,
        "no-new-symbol": 2,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 1,
        "no-path-concat": 2,
        "no-plusplus": 0,
        "no-process-env": 2,
        "no-process-exit": 2,
        "no-proto": 0,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-restricted-imports": 2,
        "no-restricted-modules": 2,
        "no-restricted-syntax": 2,
        "no-return-assign": 1,
        "no-script-url": 2,
        "no-self-assign": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-whitespace-before-property": 1,
        "no-spaced-func": 0,
        "no-sparse-arrays": 2,
        "no-sync": 2,
        "no-ternary": 0,
        "no-trailing-spaces": [1, {
                "skipBlankLines": true
            }],
        "no-this-before-super": 2,
        "no-throw-literal": 0,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-undefined": 0,
        "no-unexpected-multiline": 2,
        "no-underscore-dangle": 0,
        "no-unmodified-loop-condition": 0,
        "no-unneeded-ternary": 1,
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "no-unused-labels": 2,
        "no-unused-vars": 1,
        "no-use-before-define": 2,
        "no-useless-call": 2,
        "no-useless-concat": 1,
        "no-useless-constructor": 2,
        "no-void": 2,
        "no-var": 0,
        "no-warning-comments": 0,
        "no-with": 2,
        "no-magic-numbers": 0,
        "array-bracket-spacing": [1, "always", {
                "singleValue": false
            }],
        "array-callback-return": 0,
        "arrow-body-style": 2,
        "arrow-parens": 2,
        "arrow-spacing": 2,
        "accessor-pairs": 2,
        "block-scoped-var": 1,
        "block-spacing": 1,
        "brace-style": [1, "1tbs", {
                "allowSingleLine": true
            }],
        "callback-return": 2,
        "camelcase": 1,
        "comma-dangle": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "complexity": 0,
        "computed-property-spacing": 0,
        "consistent-return": 0,
        "consistent-this": [1, "that", "me"],
        "constructor-super": 2,
        "curly": 1,
        "default-case": 1,
        "dot-location": 2,
        "dot-notation": 2,
        "eol-last": 2,
        "eqeqeq": 2,
        "func-names": 0,
        "func-style": 0,
        "generator-star-spacing": 2,
        "global-require": 0,
        "guard-for-in": 1,
        "handle-callback-err": 2,
        "id-length": 0,
        "indent": 0,
        "init-declarations": 0,
        "jsx-quotes": 2,
        "key-spacing": [1, {
                "mode": "minimum",
                "align": "colon"}],
        "keyword-spacing": 1,
        "lines-around-comment": 1,
        "max-depth": [1, 10],
        "max-len": 0,
        "max-nested-callbacks": 2,
        "max-params": [1, 8],
        "max-statements": [1, 100],
        "new-cap": 2,
        "new-parens": 2,
        "newline-after-var": 0,
        "newline-per-chained-call": 0,
        "object-curly-spacing": [1, "always"],
        "object-shorthand": 0,
        "one-var": [1, "always"],
        "one-var-declaration-per-line": 0,
        "operator-assignment": 1,
        "operator-linebreak": [2, "after", {
                "overrides": {
                    "?": "ignore",
                    ":": "ignore"
                }
            }],
        "padded-blocks": 0,
        "prefer-arrow-callback": 0,
        "prefer-const": 2,
        "prefer-reflect": 0,
        "prefer-rest-params": 0,
        "prefer-spread": 0,
        "prefer-template": 0,
        "quote-props": 0,
        "quotes": 0,
        "radix": 0,
        "id-match": 2,
        "id-blacklist": 2,
        "require-jsdoc": 0,
        "require-yield": 2,
        "semi": 2,
        "semi-spacing": 1,
        "sort-vars": 0,
        "sort-imports": 2,
        "space-before-blocks": 1,
        "space-before-function-paren": 0,
        "space-in-parens": 1,
        "space-infix-ops": 1,
        "space-unary-ops": 2,
        "spaced-comment": 0,
        "strict": 0,
        "template-curly-spacing": 2,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "vars-on-top": 0,
        "wrap-iife": [2, "any"],
        "wrap-regex": 1,
        "yield-star-spacing": 2,
        "yoda": 0
    },
    "env": {
        "browser": true,
        "node": true
    }
}
